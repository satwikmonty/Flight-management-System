{
  "compilerOptions": {
     "esModuleInterop": true,
    "allowJs": true,
    "checkJs": false,
    "moduleResolution": "Node",
    "module": "ESNext",
    "target": "ES2020",
    "strict": true,
    "verbatimModuleSyntax": true,
    "skipLibCheck": true,
    "types": [
      "node"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "outDir": "./dist"
  },
  "exclude": ["node_modules"]
}
